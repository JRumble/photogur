
<h1>Pictures (<%= @pictures.length %>)</h1><br>

<%= link_to '<button type="button">New Picture</button>'.html_safe, "/pictures/new" %>

<br><br><br>

<% @pictures.each do |picture| %>
  <img src="<%= picture.url %>">
   <p><%= picture.title %>, <%= picture.artist %></p>
    <%= link_to image_tag(picture.url), "/pictures/#{picture.id}" %>
<% end %>

 
 